start () {

    typedef struct{ 
        float weight;  
        int age;   
        float height;
    } Pessoa;

    var {
        Pessoa joao, maria; 
        Pessoa[] population; 
    }

    joao.age = 15;
    joao.weight = 60.5;
    joao.height = 1.75;

    population[4].age = 23;
    population[4].weight = 75.3;
    population[4].height = 1.89;

    maria = population[4];

}